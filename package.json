{
  "name": "clearmix",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jbbelcher53/clearmix"
  },
  "scripts": {
    "start": "webpack serve --config build-utils/webpack.config.js --env env=dev",
    "build": "webpack --config build-utils/webpack.config.js --env env=prod",
    "prettify": "yarn prettier \"**/*.*(js|jsx|ts|tsx)\" --ignore-path=.prettierignore --write",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx && yarn prettify",
    "lint-fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix && yarn prettify",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "@types/node": "^16.6.2",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "autoprefixer": "^10.2.6",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-eslint": "^3.0.9",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "dotenv-webpack": "^7.0.3",
    "esbuild-loader": "^2.15.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^3.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": ">=6",
    "lint-staged": ">=11",
    "mini-css-extract-plugin": "^2.2.0",
    "msw": "^0.35.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "react-refresh": "^0.10.0",
    "semantic-release": "^17.4.4",
    "style-loader": "^3.2.1",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.8.0",
    "webpack-remote-types-plugin": "^0.2.6"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint src --ext .js,.jsx,.ts,.tsx --fix",
      "eslint src --ext .js,.jsx,.ts,.tsx",
      "prettier \"**/*.*(js|jsx|ts|tsx)\" --ignore-path=.prettierignore --write"
    ],
    "*.json": "prettier \"**/*.*(json)\" --ignore-path=.prettierignore --write"
  },
  "msw": {
    "workerDirectory": "build-utils/dist"
  }
}
